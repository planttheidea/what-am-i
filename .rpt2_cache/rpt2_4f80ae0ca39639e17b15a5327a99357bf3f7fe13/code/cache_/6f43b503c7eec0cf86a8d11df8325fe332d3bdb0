{"code":"/* ---------------- STRINGS ---------------- */\r\nexport var WHITESPACE_REGEXP = /^\\s\\s*/;\r\nexport var DATA_URL_REGEXP = /^\\s*data:([a-z]+\\/[a-z]+(;[a-z\\-]+\\=[a-z\\-]+)?)?(;base64)?,[a-z0-9\\!\\$\\&\\'\\,\\(\\)\\*\\+\\,\\;\\=\\-\\.\\_\\~\\:\\@\\/\\?\\%\\s]*\\s*$/i; // eslint-disable-line\r\nexport var DOUBLE_BYTE_REGEXP = /[^\\u0000-\\u00ff]/;\r\nexport var HTML_REGEXP = /<(\\\"[^\\\"]*\\\"|'[^']*'|[^'\\\">])*>/;\r\n/**\r\n * All necessary values for computation of getArrayOfWords is taken from lodash,\r\n * found at https://raw.githubusercontent.com/lodash/lodash/master/lodash.js\r\n */\r\nexport var RS_ASTRAL_RANGE = '\\\\ud800-\\\\udfff';\r\nexport var RS_COMBO_MARKS_RANGE = '\\\\u0300-\\\\u036f\\\\ufe20-\\\\ufe23';\r\nexport var RS_COMBO_SYMBOLS_RANGE = '\\\\u20d0-\\\\u20f0';\r\nexport var RS_DINGBAT_RANGE = '\\\\u2700-\\\\u27bf';\r\nexport var RS_LOWER_RANGE = 'a-z\\\\xdf-\\\\xf6\\\\xf8-\\\\xff';\r\nexport var RS_MATH_OP_RANGE = '\\\\xac\\\\xb1\\\\xd7\\\\xf7';\r\nexport var RS_NON_CHAR_RANGE = '\\\\x00-\\\\x2f\\\\x3a-\\\\x40\\\\x5b-\\\\x60\\\\x7b-\\\\xbf';\r\nexport var RS_QUOTE_RANGE = '\\\\u2018\\\\u2019\\\\u201c\\\\u201d';\r\nexport var RS_SPACE_RANGE = ' \\\\t\\\\x0b\\\\f\\\\xa0\\\\ufeff\\\\n\\\\r\\\\u2028\\\\u2029\\\\u1680\\\\u180e\\\\u2000\\\\u2001\\\\u2002\\\\u2003\\\\u2004\\\\u2005\\\\u2006\\\\u2007\\\\u2008\\\\u2009\\\\u200a\\\\u202f\\\\u205f\\\\u3000';\r\nexport var RS_UPPER_RANGE = 'A-Z\\\\xc0-\\\\xd6\\\\xd8-\\\\xde';\r\nexport var RS_VAR_RANGE = '\\\\ufe0e\\\\ufe0f';\r\nexport var RS_BREAK_RANGE = RS_MATH_OP_RANGE + RS_NON_CHAR_RANGE + RS_QUOTE_RANGE + RS_SPACE_RANGE;\r\nexport var RS_BREAK = \"[\" + RS_BREAK_RANGE + \"]\";\r\nexport var RS_COMBO = \"[\" + (RS_COMBO_MARKS_RANGE + RS_COMBO_SYMBOLS_RANGE) + \"]\";\r\nexport var RS_DIGITS = '\\\\d+';\r\nexport var RS_DINGBAT = \"[\" + RS_DINGBAT_RANGE + \"]\";\r\nexport var RS_LOWER = \"[\" + RS_LOWER_RANGE + \"]\";\r\nexport var RS_MISC = \"[^\" + (RS_ASTRAL_RANGE +\r\n    RS_BREAK_RANGE +\r\n    RS_DIGITS +\r\n    RS_DINGBAT_RANGE +\r\n    RS_LOWER_RANGE +\r\n    RS_UPPER_RANGE) + \"]\";\r\nexport var RS_FITZ = '\\\\ud83c[\\\\udffb-\\\\udfff]';\r\nexport var RS_MODIFIER = \"(?:\" + RS_COMBO + \"|\" + RS_FITZ + \")\";\r\nexport var RS_NON_ASTRAL = \"[^\" + RS_ASTRAL_RANGE + \"]\";\r\nexport var RS_REGIONAL = '(?:\\\\ud83c[\\\\udde6-\\\\uddff]){2}';\r\nexport var RS_SURR_PAIR = '[\\\\ud800-\\\\udbff][\\\\udc00-\\\\udfff]';\r\nexport var RS_UPPER = \"[\" + RS_UPPER_RANGE + \"]\";\r\nexport var RS_ZWJ = '\\\\u200d';\r\nexport var RS_LOWER_MISC = \"(?:\" + RS_LOWER + \"|\" + RS_MISC + \")\";\r\nexport var RS_UPPER_MISC = \"(?:\" + RS_UPPER + \"|\" + RS_MISC + \")\";\r\nexport var RE_OPT_MOD = RS_MODIFIER + \"?\";\r\nexport var RS_OPT_VAR = \"[\" + RS_VAR_RANGE + \"]?\";\r\nexport var RS_OPT_JOIN = \"(?:\" + RS_ZWJ + \"(?:\" + [\r\n    RS_NON_ASTRAL,\r\n    RS_REGIONAL,\r\n    RS_SURR_PAIR,\r\n].join('|') + \")\" + (RS_OPT_VAR + RE_OPT_MOD) + \")*\";\r\nexport var RS_SEQ = RS_OPT_VAR + RE_OPT_MOD + RS_OPT_JOIN;\r\nexport var RS_EMOJI = \"(?:\" + [RS_DINGBAT, RS_REGIONAL, RS_SURR_PAIR].join('|') + \")\" + RS_SEQ;\r\nexport var HAS_COMPLEX_WORD = /[a-z][A-Z]|[A-Z]{2,}[a-z]|[0-9][a-zA-Z]|[a-zA-Z][0-9]|[^a-zA-Z0-9 ]/;\r\nexport var COMPLEX_WORD = RegExp([\r\n    RS_UPPER + \"?\" + RS_LOWER + \"+(?=\" + [RS_BREAK, RS_UPPER, '$'].join('|') + \")\",\r\n    RS_UPPER_MISC + \"+(?=\" + [RS_BREAK, RS_UPPER + RS_LOWER_MISC, '$'].join('|') + \")\",\r\n    RS_UPPER + \"?\" + RS_LOWER_MISC + \"+\",\r\n    RS_UPPER + \"+\",\r\n    RS_DIGITS,\r\n    RS_EMOJI,\r\n].join('|'), 'g');\r\nexport var SIMPLE_WORD = /[a-zA-Z0-9]+/g;\r\n/* ---------------- ENVIRONMENT ---------------- */\r\n/**\r\n * Make sure window and navigator exists, else all will blow up\r\n */\r\nexport var HAS_WINDOW = typeof window !== 'undefined';\r\nexport var NAVIGATOR_IN_WINDOW = HAS_WINDOW && 'navigator' in window;\r\n/**\r\n * Returns property from navigator object exists, else returns false\r\n *\r\n * @param {string} property\r\n * @returns {boolean|string}\r\n */\r\nexport var hasNavigatorProperty = function (property) {\r\n    return NAVIGATOR_IN_WINDOW &&\r\n        property in navigator &&\r\n        navigator[property].toLowerCase();\r\n};\r\n/**\r\n * Used in pretty much all the checkers\r\n */\r\nexport var USER_AGENT = hasNavigatorProperty('userAgent');\r\nexport var VENDOR = hasNavigatorProperty('vendor');\r\nexport var APP_VERSION = hasNavigatorProperty('appVersion');\r\n/**\r\n * RegExps used for testing\r\n */\r\nexport var ANDROID_REGEXP = /android/i;\r\nexport var APPLE_COMPUTER_REGEXP = /apple computer/i;\r\nexport var APPLE_WEBKIT_REGEXP = /applewebkit/i;\r\nexport var BB10_REGEXP = /BB10/i;\r\nexport var BLACKBERRY_REGEXP = /blackberry/i;\r\nexport var CHROME_REGEXP = /chrome/i;\r\nexport var CHROME_OR_CHROMIUM_REGEXP = /chrome|chromium/i;\r\nexport var EDGE_REGEXP = /edge/i;\r\nexport var FIREFOX_REGEXP = /firefox/i;\r\nexport var GECKO_REGEXP = /gecko/i;\r\nexport var GOOGLE_INC_REGEXP = /google inc/i;\r\nexport var IPAD_REGEXP = /ipad/i;\r\nexport var IPHONE_REGEXP = /iphone/i;\r\nexport var IPOD_REGEXP = /ipod/i;\r\nexport var LIKE_GECKO_REGEXP = /like gecko/i;\r\nexport var LINUX_REGEXP = /linux/i;\r\nexport var MAC_REGEXP = /mac/i;\r\nexport var MOBILE_REGEXP = /mobile/i;\r\nexport var MSIE_REGEXP = /msie/i;\r\nexport var OPERA_LEGACY_REGEXP = /^Opera\\//;\r\nexport var OPERA_MODERN_REGEXP = /\\x20OPR\\//;\r\nexport var PHONE_REGEXP = /phone/i;\r\nexport var RV11_REGEXP = /rv:11/;\r\nexport var SAFARI_REGEXP = /safari/i;\r\nexport var TOUCH_REGEXP = /touch/i;\r\nexport var TRIDENT_REGEXP = /trident/i;\r\nexport var UNIX_REGEXP = /X11/;\r\nexport var WIN_REGEXP = /win/i;\r\nexport var WINDOWS_NT_REGEXP = /windows nt/i;\r\n/* ---------------- OBJECT ---------------- */\r\nexport var NODE_LIST_REGEXP = /^\\[object (HTMLCollection|NodeList)\\]$/;\r\n//# sourceMappingURL=regexps.js.map","map":"{\"version\":3,\"file\":\"regexps.js\",\"