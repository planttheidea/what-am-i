{"code":"import { isNumber } from './types';\r\nexport function isAbove(value, min) {\r\n    if (!isNumber(value) || !isNumber(min)) {\r\n        return false;\r\n    }\r\n    return value > min;\r\n}\r\nexport function isBelow(value, min) {\r\n    if (!isNumber(value) || !isNumber(min)) {\r\n        return false;\r\n    }\r\n    return value < min;\r\n}\r\nexport function isBetween(value, min, max) {\r\n    return (isNumber(value)\r\n        && isNumber(min)\r\n        && isNumber(max)\r\n        && value > min\r\n        && value < max);\r\n}\r\nexport function isEven(value) {\r\n    return isNumber(value) && value % 2 === 0;\r\n}\r\nexport function isNegative(value) {\r\n    return isNumber(value) && value < 0;\r\n}\r\nexport function isOdd(value) {\r\n    return isNumber(value) && value % 2 === 1;\r\n}\r\nexport function isPositive(value) {\r\n    return isNumber(value) && value > 0;\r\n}\r\nexport function isWithin(value, min, max) {\r\n    return (isNumber(value)\r\n        && isNumber(min)\r\n        && isNumber(max)\r\n        && value >= min\r\n        && value <= max);\r\n}\r\nexport function isZero(value) {\r\n    return isNumber(value) && value === 0;\r\n}\r\n//# sourceMappingURL=math.js.map","map":"{\"version\":3,\"file\":\"math.js\",\"sourceRoot\":\"\",\"sources\":[\"../../src/math.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,SAAS,CAAC;AAEnC,MAAM,UAAU,OAAO,CAAC,KAAa,EAAE,GAAW;IAChD,IAAI,CAAC,QAAQ,CAAC,KAAK,CAAC,IAAI,CAAC,QAAQ,CAAC,GAAG,CAAC,EAAE;QACtC,OAAO,KAAK,CAAC;KACd;IAED,OAAO,KAAK,GAAG,GAAG,CAAC;AACrB,CAAC;AAED,MAAM,UAAU,OAAO,CAAC,KAAa,EAAE,GAAW;IAChD,IAAI,CAAC,QAAQ,CAAC,KAAK,CAAC,IAAI,CAAC,QAAQ,CAAC,GAAG,CAAC,EAAE;QACtC,OAAO,KAAK,CAAC;KACd;IAED,OAAO,KAAK,GAAG,GAAG,CAAC;AACrB,CAAC;AAED,MAAM,UAAU,SAAS,CAAC,KAAa,EAAE,GAAW,EAAE,GAAW;IAC/D,OAAO,CACL,QAAQ,CAAC,KAAK,CAAC;WACZ,QAAQ,CAAC,GAAG,CAAC;WACb,QAAQ,CAAC,GAAG,CAAC;WACb,KAAK,GAAG,GAAG;WACX,KAAK,GAAG,GAAG,CACf,CAAC;AACJ,CAAC;AAED,MAAM,UAAU,MAAM,CAAC,KAAa;IAClC,OAAO,QAAQ,CAAC,KAAK,