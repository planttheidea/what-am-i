{"code":"import { isArray } from './types';\r\nfunction checkSortedAscending(array) {\r\n    var length = array.length;\r\n    for (var index = 0; index < length; ++index) {\r\n        if (index + 1 < length && array[index] > array[index + 1]) {\r\n            return false;\r\n        }\r\n    }\r\n    return true;\r\n}\r\nfunction checkSortedDescending(array) {\r\n    for (var index = array.length - 1; index > 0; --index) {\r\n        if (array[index] > array[index - 1]) {\r\n            return false;\r\n        }\r\n    }\r\n    return true;\r\n}\r\nexport function isArraySorted(object) {\r\n    return (isArray(object)\r\n        && (checkSortedAscending(object) || checkSortedDescending(object)));\r\n}\r\nexport function isArraySortedAsc(object) {\r\n    return isArray(object) && checkSortedAscending(object);\r\n}\r\nexport function isArraySortedDesc(object) {\r\n    return isArray(object) && checkSortedDescending(object);\r\n}\r\nexport function isInArray(object, item) {\r\n    return isArray(object) && object.indexOf(item) !== -1;\r\n}\r\n//# sourceMappingURL=array.js.map","map":"{\"version\":3,\"file\":\"array.js\",\"sourceRoot\":\"\",\"sources\":[\"../../src/array.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,OAAO,EAAE,MAAM,SAAS,CAAC;AAElC,SAAS,oBAAoB,CAAC,KAAY;IAChC,IAAA,qBAAM,CAAW;IAEzB,KAAK,IAAI,KAAK,GAAG,CAAC,EAAE,KAAK,GAAG,MAAM,EAAE,EAAE,KAAK,EAAE;QAC3C,IAAI,KAAK,GAAG,CAAC,GAAG,MAAM,IAAI,KAAK,CAAC,KAAK,CAAC,GAAG,KAAK,CAAC,KAAK,GAAG,CAAC,CAAC,EAAE;YACzD,OAAO,KAAK,CAAC;SACd;KACF;IAED,OAAO,IAAI,CAAC;AACd,CAAC;AAED,SAAS,qBAAqB,CAAC,KAAY;IACzC,KAAK,IAAI,KAAK,GAAG,KAAK,CAAC,MAAM,GAAG,CAAC,EAAE,KAAK,GAAG,CAAC,EAAE,EAAE,KAAK,EAAE;QACrD,IAAI,KAAK,CAAC,KAAK,CAAC,GAAG,KAAK,CAAC,KAAK,GAAG,CAAC,CAAC,EAAE;YACnC,OAAO,KAAK,CAAC;SACd;KACF;IAED,OAAO,IAAI,CAAC;AACd,CAAC;AAED,MAAM,UAAU,aAAa,CAAC,MAAa;IACzC,OAAO,CACL,OAAO,CAAC,MAAM,CAAC;WACZ,CAAC,oBAAoB,CAAC,MAAM,CAAC,IAAI,qBAAqB,CAAC,MAAM,CAAC,CAAC,CACnE,CAAC;AACJ,CAAC;AAED,MAAM,UAAU,gBAAgB,CAAC,MAAa;IAC5C,OAAO,OAAO,CAAC,