{"code":"import { isWithin } from './math';\r\nimport { isDate, isNaN, isNumber, isString } from './types';\r\n/**\r\n * String values for the days of the week\r\n */\r\nvar DAY_STRINGS = [\r\n    'sunday',\r\n    'monday',\r\n    'tuesday',\r\n    'wednesday',\r\n    'thursday',\r\n    'friday',\r\n    'saturday',\r\n];\r\n/**\r\n * String values for the months of the year\r\n */\r\nvar MONTH_STRINGS = [\r\n    'january',\r\n    'february',\r\n    'march',\r\n    'april',\r\n    'may',\r\n    'june',\r\n    'july',\r\n    'august',\r\n    'september',\r\n    'october',\r\n    'november',\r\n    'december',\r\n];\r\nexport function isDay(date, dayString) {\r\n    if (dayString === void 0) { dayString = DAY_STRINGS[new Date().getDay()]; }\r\n    return (isDate(date)\r\n        && isString(dayString)\r\n        && dayString.toLowerCase() === DAY_STRINGS[date.getDay()]);\r\n}\r\nexport function isDaylightSavingsTime(date) {\r\n    if (!isDate(date)) {\r\n        return false;\r\n    }\r\n    var year = date.getFullYear();\r\n    var january = new Date(year, 0, 1);\r\n    var july = new Date(year, 6, 1);\r\n    return (date.getTimezoneOffset()\r\n        < Math.max(january.getTimezoneOffset(), july.getTimezoneOffset()));\r\n}\r\nexport function isInDateRange(date, minDate, maxDate) {\r\n    if (!isDate(date) || !isDate(minDate) || !isDate(maxDate)) {\r\n        return false;\r\n    }\r\n    var time = +date;\r\n    return time >= +minDate && time <= +maxDate;\r\n}\r\nexport function isInLastMonth(date) {\r\n    var today = new Date();\r\n    var oneMonthAgo = new Date(new Date().setMonth(new Date().getMonth() - 1));\r\n    return isInDateRange(date, oneMonthAgo, today);\r\n}\r\nexport function isInLastWeek(date) {\r\n    var today = new Date();\r\n    var oneWeekAgo = new Date(new Date().setDate(new Date().getDate() - 7));\r\n    return isInDateRange(date, oneWeekAgo, today);\r\n}\r\nexport function isInLastYear(date) {\r\n    var today = new Date();\r\n    var oneYearAgo = new Date(new Date().setFullYear(new Date().getFullYear() - 1));\r\n    return isInDateRange(date, oneYearAgo, today);\r\n}\r\nexport function isInNextMonth(date) {\r\n    var today = new Date();\r\n    var oneMonthFromNow = new Date(new Date().setMonth(new Date().getMonth() + 1));\r\n    return isInDateRange(date, today, oneMonthFromNow);\r\n}\r\nexport function isInNextWeek(date) {\r\n    var today = new Date();\r\n    var oneWeekFromNow = new Date(new Date().setDate(new Date().getDate() + 7));\r\n    return isInDateRange(date, today, oneWeekFromNow);\r\n}\r\nexport function isInNextYear(date) {\r\n    var today = new Date();\r\n    var oneYearFromNow = new Date(new Date().setFullYear(new Date().getFullYear() + 1));\r\n    return isInDateRange(date, today, oneYearFromNow);\r\n}\r\nexport function isLeapYear(date) {\r\n    var epoch = isDate(date) ? +date : date;\r\n    return (isNumber(epoch)\r\n        && ((epoch % 4 === 0 && epoch % 100 !== 0) || epoch % 400 === 0));\r\n}\r\nexport function isMonth(date, monthString) {\r\n    if (monthString === void 0) { monthString = MONTH_STRINGS[new Date().getMonth()]; }\r\n    return (isDate(date)\r\n        && isString(monthString)\r\n        && monthString.toLowerCase() === MONTH_STRINGS[date.getMonth()]);\r\n}\r\nexport function isPast(date) {\r\n    return isDate(date) && +date < Date.now();\r\n}\r\nexport function isQuarterOfYear(date, quarter) {\r\n    return (isDate(date)\r\n        && isWithin(quarter, 1, 4)\r\n        && quarter === Math.floor((date.getMonth() + 3) / 3));\r\n}\r\nexport function isToday(date, today) {\r\n    if (today === void 0) { today = new Date(); }\r\n    return (isDate(date)\r\n        && isDate(today)\r\n        && date.toDateString() === today.toDateString());\r\n}\r\nexport function isTomorrow(date, tomorrow) {\r\n    if (tomorrow === void 0) { tomorrow = new Date(new Date().setDate(new Date().getDate() + 1)); }\r\n    return isToday(date, tomorrow);\r\n}\r\nexport function isWeekday(date) {\r\n    return isDate(date) && isWithin(date.getDay(), 1, 5);\r\n}\r\nexport function isWeekend(date) {\r\n    if (!isDate(date)) {\r\n        return false;\r\n    }\r\n    var day = date.getDay();\r\n    return day === 0 || day === 6;\r\n}\r\nexport function isYear(date, year) {\r\n    if (year === void 0) { year = new Date().getFullYear(); }\r\n    return (isDate(date)\r\n        && isNumber(year)\r\n        && !isNaN(year)\r\n        && year === date.getFullYear());\r\n}\r\nexport function isYesterday(date, yesterday) {\r\n    if (yesterday === void 0) { yesterday = new Date(new Date().setDate(new Date().getDate() - 1)); }\r\n    return isToday(date, yesterday);\r\n}\r\n//# sourceMappingURL=temporal.js.map","map":"{\"version\":3,\"file\":\"temporal.js\",\"sourceRoot\":\"\",\"sources\":[\"../../src/temporal.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,QAAQ,CAAC;AAClC,OAAO,EAAE,MAAM,EAAE,KAAK,EAAE,QAAQ,EAAE,QAAQ,EAAE,MAAM,SAAS,CAAC;AAE5D;;GAEG;AACH,IAAM,WAAW,GAAG;IAClB,QAAQ;IACR,QAAQ;IACR,SAAS;IACT,WAAW;IACX,UAAU;IACV,QAAQ;IACR,UAAU;CACX,CAAC;AAEF;;GAEG;AACH,IAAM,aAAa,GAAG;IACpB,SAAS;IACT,UAAU;IACV,OAAO;IACP,OAAO;IACP,KAAK;IACL,MAAM;IACN,MAAM;IACN,QAAQ;IACR,WAAW;IACX,SAAS;IACT,UAAU;IACV,UAAU;CACX,CAAC;AAEF,MAAM,UAAU,KAAK,CACnB,IAAU,EACV,SAAoD;IAApD,0BAAA,EAAA,YAAoB,WAAW,CAAC,IAAI,IAAI,EAAE,CAAC,MAAM,EAAE,CAAC;IAEpD,OAAO,CACL,MAAM,CAAC,IAAI,CAAC;WACT,QAAQ,CAAC,SAAS,CAAC;WACnB,SAAS,CAAC,WAAW,EAAE,KAAK,WAAW,CAAC,IAAI,CAAC,MAAM,EAAE,CAAC,CAC1D,CAAC;AACJ,CAAC;AAED,MAAM,UAAU,qBAAqB,CAAC,IAAU;IAC9C,IAAI,CAAC,MAAM,CAAC,IAAI,CAAC,EAAE;QACjB,OAAO,KAAK,CAAC;KACd;IAED,IAAM,IAAI,GAAG,IAAI,CAAC,WAAW,EAAE,CAAC;IAChC,IAAM,OAAO,GAAG,IAAI,IAAI,CAAC,IAAI,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC;IACrC,IAAM,IAAI,GAAG,IAAI,IAAI,CAAC,IAAI,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC;IAElC,OAAO,CACL,IAAI,CAAC,iBAAiB,EAAE;UACtB,IAAI,CAAC,GAAG,CAAC,OAAO,CAAC,iBAAiB,EAAE,EAAE,IAAI,CAAC,iBAAiB,EAAE,CAAC,CAClE,CAAC;AACJ,CAAC;AAED,MAAM,UAAU,aAAa,CAC3B,IAAU,EACV,OAAa,EACb,OAAa;IAEb,IAAI,CAAC,MAAM