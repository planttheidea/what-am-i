tring === toSnakeCase(string);\r\n}\r\nexport function isScreamingSnakeCase(string) {\r\n    return isSnakeCase(string) && string === string.toUpperCase();\r\n}\r\nexport function isStartingWith(string, starting) {\r\n    return (isString(string) && isString(starting) && string.indexOf(starting) === 0);\r\n}\r\nexport function isSpace(string) {\r\n    return isString(string) && string === ' ';\r\n}\r\nexport function isUpperCase(string) {\r\n    return isString(string) && string === string.toUpperCase();\r\n}\r\nexport function isWhitespace(string) {\r\n    return isString(string) && WHITESPACE.test(string);\r\n}\r\n//# sourceMappingURL=string.js.map","map":"{\"version\":3,\"file\":\"string.js\",\"sourceRoot\":\"\",\"sources\":[\"../../src/string.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,WAAW,EAAE,IAAI,EAAE,UAAU,EAAE,MAAM,qBAAqB,CAAC;AAC9E,OAAO,EACL,eAAe,EACf,WAAW,EACX,WAAW,EACX,YAAY,EACZ,WAAW,GACZ,MAAM,mBAAmB,CAAC;AAE3B,OAAO,EAAE,OAAO,EAAE,MAAM,UAAU,CAAC;AACnC,OAAO,EAAE,QAAQ,EAAE,MAAM,SAAS,CAAC;AAEnC,MAAM,UAAU,WAAW,CAAC,MAAc;IACxC,OAAO,QAAQ,CAAC,MAAM,CAAC,IAAI,MAAM,KAAK,WAAW,CAAC,MAAM,CAAC,CAAC;AAC5D,CAAC;AAED,MAAM,UAAU,aAAa,CAAC,MAAc;IAC1C,IAAI,CAAC,QAAQ,CAAC,MAAM,CAAC,EAAE;QACrB,OAAO,KAAK,CAAC;KACd;IAED,IAAM,KAAK,GAAG,eAAe,CAAC,MAAM,CAAC,CAAC;IACtC,IAAM,WAAW,GAAG,KAAK,CAAC,GAAG,CAC3B,UAAA,IAAI,IAAI,OAAA,KAAG,IAAI,CAAC,CAAC,CAAC,CAAC,WAAW,EAAE,GAAG,IAAI,CAAC,KAAK,CAAC,CAAC,CAAG,EAA1C,CAA0C,CACnD,CAAC;IAEF,OAAO,OAAO,CAAC,KAAK,EAAE,WAAW,CAAC,CAAC;AACrC,CAAC;AAED,MAAM,UAAU,SAAS,CAAC,MAAc;IACtC,OAAO,QAAQ,CAAC,MAAM,CAAC,IAAI,QAAQ,CAAC,IAAI,CAAC,MAAM,CAAC,CAAC;AACnD,CAAC;AAED,MAAM,UAAU,YAAY,CAAC,MAAc;IACzC,OAAO,QAAQ,CAAC,MAAM,CAAC,IAAI,WAAW,CAAC,IAAI,CAAC,MAAM,CAAC,CAAC;AACtD,CAAC;AAED,MAAM,UAAU,YAAY,CAAC,MAAc,EAAE,MAAc;IACzD,IAAI,CAAC,QAAQ,CAAC,MAAM,CAAC,IAAI,CAAC,QAAQ,CAAC,MAAM,CAAC,EAAE;QAC1C,OAAO,KAAK,CAAC;KACd;IAED,IAAM,KAAK,GAAG,MAAM,CAAC,OAAO,CAAC,MAAM,CAAC,CAAC;IAErC,OAAO,KAAK,KAAK,CAAC,CAAC,IAAI,KAAK,KAAK,MAAM,CAAC,MAAM,GAAG,MAAM,CAAC,MAAM,CAAC